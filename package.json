{
    "name": "filenamecomplete",
    "displayName": "FileNameComplete",
    "description": "Provides the name of the current typescript file in intellisense",
    "version": "0.0.1",
    "publisher": "Dan Young",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other", "Languages"
    ],
    "activationEvents": [
        "onLanguage:typescript"
    ],
    "main": "./out/src/extension",
    "contributes": {
       
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}